<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE simulation SYSTEM "simulation.dtd"> 
<simulation finalinst="1000" initpop="500" maxpop="500" comfortsens="3"> 
   <grid colsnb="5" rowsnb="5"/> 
   <initialpoint xinitial="1" yinitial="2"/> 
   <finalpoint xfinal="3" yfinal="3"/>
   <specialcostzones num="2">
      <zone xinitial="1" yinitial="1" xfinal="4" yfinal="4">4</zone>
      <zone xinitial="1" yinitial="1" xfinal="4" yfinal="4">8</zone>
   </specialcostzones>
   <obstacles num="3">
      <obstacle xpos="3" ypos="2"/>
      <obstacle xpos="2" ypos="3"/>
      <obstacle xpos="4" ypos="3"/>
   </obstacles>
   <events>
      <death param="100"/>
      <reproduction param="10"/>
      <move param="1"/>
   </events>
</simulation>